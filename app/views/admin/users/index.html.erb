<div class="container mt-5">
  <div class="row">
    <div class="col-md-6 offset-md-3">
      <h1 class="display-6 text-center mb-4">登録ユーザー</h1>
      <ul class="list-group">
        <% @users.each do |user| %>
          <li class="list-group-item">
            <%= link_to user.name, admin_user_path(user) %>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

